<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentStart="true"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/ivsimpsons1"
        android:layout_width="200dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="false"
        android:layout_centerHorizontal="false"
        android:layout_marginStart="35dp"
        android:layout_marginTop="155dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="500dp"
        android:contentDescription="@string/the_simpsons"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ivhomer1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.357" />

    <ImageView
        android:id="@+id/ivhomer1"
        android:layout_width="200dp"
        android:layout_height="270dp"
        android:layout_alignEnd="@id/ivsimpsons1"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="135dp"
        android:layout_marginEnd="8dp"
        android:contentDescription="@string/todo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivsimpsons1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/homer_simpson" />

    <ImageView
        android:id="@+id/ivdonut"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="92dp"
        app:layout_constraintEnd_toStartOf="@+id/ivull"
        app:layout_constraintTop_toBottomOf="@+id/ivsimpsons1"
        />

    <ImageView
        android:id="@+id/ivull"
        android:layout_width="34dp"
        android:layout_height="34dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivhomer1"
        app:layout_constraintEnd_toEndOf="@+id/ivhomer1"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="@+id/ivhomer1"
        app:layout_constraintTop_toTopOf="@+id/ivhomer1"
        app:layout_constraintVertical_bias="0.443"
        />

    <ImageView
        android:id="@+id/ivengvermell"
        android:layout_width="66dp"
        android:layout_height="66dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivhomer1"
        app:layout_constraintEnd_toEndOf="@+id/ivhomer1"
        app:layout_constraintHorizontal_bias="0.414"
        app:layout_constraintStart_toStartOf="@+id/ivhomer1"
        app:layout_constraintTop_toTopOf="@+id/ivhomer1"
        app:layout_constraintVertical_bias="0.105" />

    <ImageView
        android:id="@+id/ivengverd"
        android:layout_width="56dp"
        android:layout_height="56dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivhomer1"
        app:layout_constraintEnd_toEndOf="@+id/ivhomer1"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="@+id/ivhomer1"
        app:layout_constraintTop_toTopOf="@+id/ivhomer1"
        app:layout_constraintVertical_bias="0.191" />

    <ImageView
        android:id="@+id/ivengblau"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivhomer1"
        app:layout_constraintEnd_toEndOf="@+id/ivhomer1"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="@+id/ivhomer1"
        app:layout_constraintTop_toTopOf="@+id/ivhomer1"
        app:layout_constraintVertical_bias="0.373" />

</androidx.constraintlayout.widget.ConstraintLayout>